		"@react-router/dev": "^7.7.1",
		"@tailwindcss/vite": "^4.1.4",
		"@types/node": "^20",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"daisyui": "^5.0.50",
		"tailwindcss": "^4.1.4",
		"typescript": "^5.8.3",
